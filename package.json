{
  "name": "es-style",
  "version": "0.1.10",
  "license": "MIT",
  "description": "easy stylesheet style",
  "files": [
    "dist",
    "server.js",
    "babel.js",
    "watch.js",
    "index.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "scripts": {
    "dev": "taskr",
    "release": "taskr release",
    "build": "npm run release && npm run start",
    "start": "node publish.js && cd publish && npm publish",
    "spa": "cd examples/spa && npm run dev",
    "ssr": "cd examples/ssr && npm run dev",
    "spa_build": "cd examples/spa && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Topthinking/es-style.git"
  },
  "author": "Topthinking",
  "bugs": {
    "url": "https://github.com/Topthinking/es-style/issues"
  },
  "homepage": "https://github.com/Topthinking/es-style#readme",
  "devDependencies": {
    "@taskr/babel": "^1.1.0",
    "@taskr/clear": "^1.1.0",
    "@taskr/esnext": "^1.1.0",
    "@taskr/uglify": "^1.1.0",
    "@taskr/watch": "^1.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "node-notifier": "^5.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "schema-utils": "^0.4.3",
    "style-loader": "^0.20.1",
    "styled-components": "^3.1.6",
    "styled-jsx": "^2.2.3",
    "taskr": "^1.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "chokidar": "^2.0.3",
    "cssnano": "^4.0.0-rc.2",
    "deasync": "^0.1.12",
    "fs-extra": "^5.0.0",
    "md5": "^2.2.1",
    "memory-fs": "^0.4.1",
    "mime": "^2.3.1",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "require-resolve": "^0.0.2",
    "spritesmith": "^3.3.0",
    "string-hash": "^1.1.3",
    "svg-sprite": "^1.4.0"
  }
}
