{
  "name": "es-style",
  "version": "2.1.13",
  "license": "MIT",
  "description": "easy stylesheet style",
  "files": [
    "dist",
    "server.js",
    "babel.js",
    "watch.js",
    "index.js",
    "webpack-plugin.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "dev": "taskr",
    "postinstall": "npm run release",
    "release": "taskr release",
    "build": "npm run release && npm run start",
